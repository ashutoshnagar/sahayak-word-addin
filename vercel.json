{
  "version": 2,
  "name": "sahayak-word-addin",
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/manifest.xml",
      "dest": "/public/manifest.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "/taskpane.html",
      "dest": "/public/taskpane.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/commands.html",
      "dest": "/public/commands.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/manifest.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "functions": {},
  "rewrites": []
}
